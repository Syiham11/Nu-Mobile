<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="1dp"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/activity_main"
    tools:context="com.adfin.numobile.activity.MainActivity">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:visibility="gone"
                android:id="@+id/hereWe"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
            <ImageView
                android:layout_width="230dp"
                android:layout_height="130dp"
                android:background="@drawable/numobile"
                android:id="@+id/imageView2"
                android:layout_centerHorizontal="true"
                android:layout_alignParentTop="true" />

            <!-- baris 1-->
            <Button
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="@drawable/pengurus"
                android:id="@+id/btn_pengurus"
                android:layout_marginTop="130dp"
                />
            <Button
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="@drawable/warga"
                android:id="@+id/btn_anggota"
                android:layout_marginTop="130dp"
                android:layout_marginLeft="90dp"
                />
            <Button
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="@drawable/kearsipan"
                android:id="@+id/btn_arsip"
                android:layout_marginTop="130dp"
                android:layout_marginLeft="180dp"
                />

            <Button
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="@drawable/kegiatan_dan_kejadian"
                android:id="@+id/btn_kegiatan"
                android:layout_marginTop="130dp"
                android:layout_marginLeft="270dp"

                />

            <!-- baris 2-->
            <Button
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="@drawable/pelayanan"
                android:id="@+id/btn_pelayanan"
                android:layout_marginTop="230dp"
                />

            <Button
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="@drawable/promosi"
                android:id="@+id/btn_promosi"
                android:layout_marginTop="230dp"
                android:layout_marginLeft="90dp"
                />

            <Button
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="@drawable/warung"
                android:id="@+id/btn_warung"
                android:layout_marginTop="230dp"
                android:layout_marginLeft="180dp"
                />

            <Button
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="@drawable/pasar"
                android:id="@+id/btn_pasar"
                android:layout_marginTop="230dp"
                android:layout_marginLeft="270dp"
                />

            <!-- baris 3-->
            <Button
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="@drawable/dompet"
                android:id="@+id/btn_dompet"
                android:layout_marginTop="330dp"
                />
            <Button
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="@drawable/konsultasi"
                android:id="@+id/btn_konsultasi"
                android:layout_marginTop="330dp"
                android:layout_marginLeft="90dp"
                />
            <Button
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="@drawable/doa"
                android:id="@+id/btn_doa"
                android:layout_marginTop="330dp"
                android:layout_marginLeft="180dp"
                />
            <Button
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="@drawable/desa_nu"
                android:id="@+id/btn_desa"
                android:layout_marginTop="330dp"
                android:layout_marginLeft="270dp"
                />

            <!-- baris 4-->
            <Button
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="@drawable/kesehatan"
                android:id="@+id/btn_kesehatan"
                android:layout_marginTop="430dp"
                />
            <Button
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="@drawable/pendidikan"
                android:id="@+id/btn_pendidikan"
                android:layout_marginTop="430dp"
                android:layout_marginLeft="90dp"
                />
            <Button
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="@drawable/pertanian"
                android:id="@+id/btn_pertanian"
                android:layout_marginTop="430dp"
                android:layout_marginLeft="180dp"
                />
            <Button
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="@drawable/kelautan"
                android:id="@+id/btn_kelautan"
                android:layout_marginTop="430dp"
                android:layout_marginLeft="270dp"
                />
        </RelativeLayout>

</RelativeLayout>
